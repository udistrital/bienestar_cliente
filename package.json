        "build:prod": "npm run build -- --prod --aot --configuration=production",
        "build:test": "npm run build -- --prod --aot --configuration=test",
        "build:dev": "npm run build -- --prod --aot --configuration=dev",
        "test": "jest",
        "test:ui": "npm run lint && npm run test",
